{
  "name": "my-cert-community",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
    "dev:client": "vite",
    "dev:server": "nodemon --watch server --exec ts-node server/index.ts",
    "build": "vite build",
    "preview": "vite preview",
    "deploy:s3": "aws s3 sync dist/ s3://hippo-project-frontend --delete",
    "deploy:cloudfront": "aws cloudfront create-invalidation --distribution-id E1FXL8ZNZFQFXQ --paths '/*'",
    "deploy": "node deploy.js"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.11.2",
    "@aws-sdk/client-cloudfront": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "aws-amplify": "^6.15.5",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "lucide-react": "^0.395.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.2.2",
    "vite": "^5.4.19"
  }
}
